    <script src="~/app/shopping/components/planning/planningController.js"></script>
    <script src="~/app/shopping/components/planning/createNewProductDialogController.js"></script>
    <script src="~/app/shopping/components/inStore/inStore.js"></script>
    <script src="~/app/shopping/components/inStore/inStoreController.js"></script>
    <script src="~/app/shopping/components/inStore/selectStoreController.js"></script>
