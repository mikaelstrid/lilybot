    <script src="~/app/shopping/services/profilesService.js"></script>
    <script src="~/app/shopping/services/storesService.js"></script>
    <script src="~/app/shopping/services/productsService.js"></script>
    <script src="~/app/shopping/services/itemsService.js"></script>
    <script src="~/app/shopping/components/home/home.js"></script>
    <script src="~/app/shopping/components/home/homeController.js"></script>
    <script src="~/app/shopping/components/stores/stores.js"></script>
